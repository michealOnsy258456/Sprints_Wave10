#include "Application/App.h"
#include <avr/io.h>
/*****************************************************************************************
 *                                MAIN FUNCTION                                          *
 *****************************************************************************************/

int main(void)
{
	/********************************************************************************
	 *                           SYSTEM INITIALIZATION                              *
	 ********************************************************************************/

	App_init_Without_Looping();	/* Test KEYPAD in simple application */

	/********************************************************************************
	 *                        APPLICATION	(SUPER LOOP)						    *
	 ********************************************************************************/

	while(1)
	{
		App_init();
		App_Update();
		_delay_ms(20);

	}	/* END OF SUPER LOOP */
}	/* END OF MAIN FUNCTION */
