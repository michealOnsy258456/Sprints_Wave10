/*****************************************************************************************
 *                                INCLUDES                                               *
 *****************************************************************************************/
#include"APP/APP.h"
/*****************************************************************************************
 *                                MAIN FUNCTION                                          *
 *****************************************************************************************/
int main()
{
/********************************************************************************
*                           SYSTEM INITIALIZATION                              *
********************************************************************************/
	APP_Init();
	while(1)
	{
/********************************************************************************
*                        APPLICATION	(SUPER LOOP)						    *
********************************************************************************/
		APP_Update();
	}/* END OF SUPER LOOP */
}/* END OF MAIN FUNCTION */
